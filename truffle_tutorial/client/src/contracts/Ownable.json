  "source": "pragma solidity ^0.8.0;\n\ncontract Ownable {\n\n    address payable owner;\n\n    modifier onlyOwner() {\n        require(owner == msg.sender, \"Operation denied: You are not the owner\");\n        _;\n    }\n\n    function isOnwer() public view returns(address) {\n        return owner;\n    }\n\n}",
